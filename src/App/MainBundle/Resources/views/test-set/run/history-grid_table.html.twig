<table id="history-grid-{{ testSet.id }}"
        class="table table-hover table-condensed table-striped table-responsive table-bordered">
    <thead>
        <tr>
            <th class="text-center"><i class="fontello-icon-pencil">Name</i></th>
            <th class="text-center"><i class="fontello-icon-upload-cloud"></i>Requested</th>
            <th class="text-center"><i class="fontello-icon-user"></i>By</th>
            <th class="text-center"><i class="fontello-icon-flag-checkered"></i>Status</th>
            <th class="text-center"><i class="fontello-icon-clock"></i>Duration (seconds)</th>
            <th class="text-center"><i class="fontello-icon-cloud"></i>Execution Server</th>
            <th class="text-center"><i class="fontello-icon-edit"></i>Actions</th>
        </tr>
    </thead>
    <tbody>
        {% for testSetRun in testSet.runs %}
            {% include "AppMainBundle:test-set:run/item.html.twig" with { 
                'testSetRun': testSetRun
            } %}
        {% endfor %}
    </tbody>
</table>