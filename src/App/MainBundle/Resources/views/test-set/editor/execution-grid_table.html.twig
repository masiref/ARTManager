<table id="execution-grid-{{ testSet.id }}"
        class="table table-hover table-condensed table-striped table-responsive">
    <thead>
        <tr>
            <th>Order</th>
            <th>Test name</th>
            <th>Last Run Status</th>
            <th>Last Run Request</th>
            {% if showActions %}
                <th>Actions</th>
            {% endif %}
        </tr>
    </thead>
    <tbody>
        {% for testInstance in testSet.testInstances %}
            {% include "AppMainBundle:test:instance/item.html.twig" with { 
                'testInstance': testInstance,
                'position': loop.index,
                'showActions': showActions
            } %}
        {% endfor %}
    </tbody>
</table>