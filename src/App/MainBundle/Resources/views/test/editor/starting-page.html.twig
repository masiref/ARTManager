<div class="row test-starting">
    <div class="col-md-12">
        <h3 class="text-center">
            <span class="pull-left">
                <a href="#starting-page-collapse" data-toggle="collapse" aria-expanded="false" aria-controls="starting-page-collapse">
                    <i class="fontello-icon-down-open starting-page-collapse-toggle-icon"></i></a>
            </span>
            <strong>
                <i class="fontello-icon-to-start"></i> Starting page
            </strong>
            <small>
                <span id="starting-page-name">
                    {% if test.startingPage %}{{ test.startingPage }}{% endif %}
                </span>
            </small>
            <span class="pull-right">
                <a href="#starting-page-collapse" data-toggle="collapse" aria-expanded="false" aria-controls="starting-page-collapse">
                    <i class="fontello-icon-down-open starting-page-collapse-toggle-icon"></i></a>
            </span>
        </h3>
    </div>
    <div class="col-md-4 col-md-offset-4">
        <div class="collapse" id="starting-page-collapse">
            <p class="text-center">
                {% set form = startingPageFormView %}
                {{ form_widget(form.startingPage) }}
            </p>
        </div>
    </div>
</div>