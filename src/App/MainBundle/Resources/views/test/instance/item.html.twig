<tr id="row-test-instance-{{ testInstance.id }}" data-position="{{ position }}">
    <td id="cell-test-instance-{{ testInstance.id }}-order">
        {{ testInstance.order }}
    </td>
    <td id="cell-test-instance-{{ testInstance.id }}-test-name">
        {{ testInstance.test }}
    </td>
    <td id="cell-test-instance-{{ testInstance.id }}-status">
        {% set lastRun = testInstance.lastRun %}
        {% if lastRun %}
            {{ lastRun.status.html|raw }}
        {% endif %}
    </td>
    <td id="cell-test-instance-{{ testInstance.id }}-last-runned">
        {% if lastRun %}
            {{ lastRun.createdAt|ago }}
        {% endif %}
    </td>
    <td id="cell-test-instance-{{ testInstance.id }}-delete-anchor">
        <a href="#" id="delete-test-instance-{{ testInstance.id }}"
           data-id="{{ testInstance.id }}" 
           data-order="{{ testInstance.order }}"
           data-test-name="{{ testInstance.test }}"
           data-toggle="tooltip" data-placement="right" title="Delete">
            <i class="fontello-icon-minus-circled"></i></a>
    </td>
</tr>